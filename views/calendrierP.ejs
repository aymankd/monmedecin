  <script>

    $(document).ready(function(){
        $("#searchin").on("input", function(){
          search = $('#searchin').val();
      if(search!='')
          $.ajax({
            url : "/recherche",
            data :{
              Stxt: search 
            },
            method:"POST",
            success : function (res) {
              $('#search_resu').html(res)
            }
          })
          else $('#search_resu').html('')
        });
    });
  </script>

<script type="text/javascript">    
  $(document).ready(function(){
          var calendar = $("#calendar").fullCalendar({  
              header: {
                  left: 'prev,next today',
                  center: 'title',
                  right: 'month,agendaWeek,agendaDay,listWeek'
              },
              locale:'fr',
              navLinks: true, 
              editable: true,
              eventLimit: true, 
              events: [

              <% if (locals.data) { 
              
                data.forEach( (row) => {
                
              %>

                  {
                      title  : '<%  __append(row.nomP) %> <%  __append(row.prenomP) %>',
                      start  : '<%  __append(row.daterdv) %>'
                      
                  },
              <%  });   %>

              <% }  %>
                  {
                      title  : 'event3',
                      start  : '2014-01-09T12:30:00',
                      allDay : false // will make the time show
                  }
              ],  // request to load current events
             
          });
          
       });
  </script>


