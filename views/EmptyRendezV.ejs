  <script type="text/javascript">

    function deleterdv(id) {
      $.ajax({
            url : "/Supprdvmedecin",
            data :{
              idu: id
            },
            method:"POST",
            success : function (res) {
             console.log(res)
             row = document.getElementById('rdv-row-'+id);
             row.remove()
            }
          })
    }

    function validaterdv(id) {
          var dateinp = document.getElementById('date-'+id)
          valdate = dateinp.value;
          if(valdate!=""){
            $.ajax({
            url : "/Confrdvmedecin",
            data :{
              idu: id,
              date:valdate
            },
            method:"POST",
            success : function (res) {
             console.log(res)
             row = document.getElementById('rdv-row-'+id);
             row.remove()
            }
          })
          }
    }



</script>
  

