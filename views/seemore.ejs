  <script>
    $(document).ready(function(){
      var id_medecin = <%  __append(med[0].Id_user) %>
        $("#rdv").click(function(){
          console.log("button clicked")
          if($("#rdv").hasClass("anl"))
          {
            $.ajax({
            url : "/rendezvous",
            data :{
              idmedecin : id_medecin,
              type : "annuler"
            },
            method:"POST",
            success : function() {
              $("#rdv").removeClass("anl")
              $("#rdv").html("Prendre un rendez vous")
            }
          })
          }else
          {
            $.ajax({
              url : "/rendezvous",
            data :{
              idmedecin : id_medecin,
              type : "demander"
            },
            method:"POST",
            success : function () {
              $("#rdv").addClass("anl")
              $("#rdv").html("annuler rendez vous")
            }
          })
          }
        });
    });

  </script>
